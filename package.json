{
  "name": "@real-world/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nest": "npx @nestjs/cli@latest",
    "nx": "nx ",
    "lint": "nx lint ",
    "dev": "nx dev ",
    "test": "nx test ",
    "build": "nx build ",
    "test:e2e": "nx test:e2e ",
    "dev:apis": "nx run-many --target=dev --projects=auth,inventory,feature-flags",
    "dev:uis": "nx run-many --target=dev --projects=store-front,warehouse",
    "lint:affected": "nx affected --target=lint",
    "test:affected": "nx affected --target=test",
    "build:affected": "nx affected --target=build",
    "test:e2e:affected": "nx affected --target=test:e2e",
    "lint:all": "nx run-many --target=lint --all",
    "test:all": "nx run-many --target=test --all",
    "build:all": "nx run-many --target=build --all",
    "test:e2e:all": "nx run-many --target=test:e2e --all"
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@nx/js": "18.3.4",
    "@nx/workspace": "18.3.4",
    "nx": "18.3.4"
  }
}
